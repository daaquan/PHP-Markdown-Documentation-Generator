{
  "name": "daaquan/php-markdown-documentation-generator",
  "description": "Command line tool for generating markdown-formatted class documentation",
  "homepage": "https://github.com/daaquan/php-markdown-documentation-generator",
  "license": "MIT",
  "authors": [{
      "name": "Victor Jonsson",
      "email": "kontakt@victorjonsson.se"
    }, {
      "name": "daaquan",
      "email": "daaquan@gmail.com"
    }],
  "require": {
    "php": ">=7.1",
    "symfony/console": "~4.0",
    "symfony/dependency-injection": "~4.0",
    "symfony/event-dispatcher": "~4.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "bin": ["bin/php-doc-md"],
  "autoload": {
    "psr-4": {
      "DocsMd\\": "src/"
    }
  }
}
